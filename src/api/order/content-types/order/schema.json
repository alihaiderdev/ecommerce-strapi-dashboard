{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "total": {
      "type": "decimal",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ]
    },
    "checkoutSession": {
      "type": "string"
    },
    "quantityWithProductIds": {
      "type": "json",
      "required": true
    }
  }
}
